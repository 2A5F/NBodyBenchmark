.icon-right-next {
    width: 50px;
    height: 50px;
    align-items: center;
    justify-content: center;
}

.icon-right-next > .item {
    width: 25px;
    height: 2px;
    background-color: rgb(200, 200, 200);
    border-radius: 1px;
}

.icon-right-next > .item.i1 {
    rotate: 45deg;
    translate: 1px -7.5px;
}

.icon-right-next > .item.i2 {
    rotate: -45deg;
    translate: 1px 7.5px;
}
